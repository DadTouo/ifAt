<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="admin" extends="struts-default" namespace="/Admin">
		<action name="*_*" class="com.ifat.action.{1}Action" method="{2}">
			<!-- 管理员操作 -->
			<result name="adminLoginSuccess">/admin/admin.jsp</result>
			<result name="adminLoginFailed">/admin/adminBaseOperation/login.jsp</result>
		</action>
	</package>
	
	<package name="sutdent" extends="struts-default" namespace="/Student">
		<action name="*_*" class="com.ifat.action.{1}Action" method="{2}">
			<!-- 管理员操作 -->
			<result name="studentLoginSuccess">/student/student.jsp</result>
			<result name="studentLoginFailed">/student/studentBaseOperation/login.jsp</result>
		</action>
	</package>
</struts>    
